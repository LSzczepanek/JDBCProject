<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>TestConnection</servlet-name>
  	<display-name>TestConnection</display-name>
  	<description></description>
  	<servlet-class>main.webapp.TestConnection</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Test</servlet-name>
  	<display-name>Test</display-name>
  	<description></description>
  	<servlet-class>main.webapp.servlets.Test</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginValidateServlet</servlet-name>
  	<display-name>LoginValidateServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.LoginValidateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TestConnection</servlet-name>
  	<url-pattern>/TestConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Test</servlet-name>
  	<url-pattern>/Test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginValidateServlet</servlet-name>
  	<url-pattern>/loginvalid</url-pattern>
  </servlet-mapping>
</web-app>



